<!-- Отчеты - Карта -->
<div id="reportKarta" ng-controller="reportKartaCtrl" class="tab-content" ng-show="global.selectedReportSubMenuItem === 6 && global.function.havePravoRead(4, 15)">

  <div class="karta-header">
    <span>Дата</span>
    <input type="date" ng-model="fromDate" />
    <span> - </span>
    <input type="date" ng-model="toDate" />
    <button ng-click="refresh()">Применить</button>
  </div>

  <div id="myMap" style="width: 800px; height: 450px"></div>
  <div class="karta-status">
    <p ng-repeat="item in status"><strong>{{item.kolvo}}</strong>&nbsp;{{item.name}}</p>
  </div>

</div>
<!-- /отчеты - карта -->