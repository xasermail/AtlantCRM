        <th>План</th>
        <th>Валовый доход</th>
        <th>Расходы</th>
      </tr>
    </thead>
    <tr ng-repeat="item in saloni">
      <td title="{{item.name + ' (' + item.m_org_id + ')'}}" class="td-saloni-first-col">{{item.adres}}</td>
      <td>{{item.napol}}</td>
      <td>{{item.novPoset}}</td>
      <td>{{item.zvonki}}</td>
      <td>{{item.sotrud}}</td>
      <td>{{item.kontakti}}</td>
      <td>{{item.prodaji}}</td>
      <td>{{item.plan}}</td>
      <td>{{item.valDohod}}</td>
      <td>{{item.rash}}</td>
    </tr>
    <tr class="saloni-itogo">
      <td>Итого:</td>
      <td>{{getSum("napol")}}</td>
      <td>{{getSum("novPoset")}}</td>
      <td>{{getSum("zvonki")}}</td>
      <td>{{getSum("sotrud")}}</td>
      <td>{{getSum("kontakti")}}</td>
      <td>{{getSum("prodaji")}}</td>
